{
  "expo": {
    "name": "Killer Pool App",
    "slug": "killerpoolapp",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "description": "Application built to keep track of scoring in the popular pool game killer",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "entryPoint": "./index.js"
  }
}
